<section id="content">
  <div class="container-fluid">
    <div class="card">
      <div class="card-body card-padding">
          <div class="form-wizard-basic fw-container">
          <ul class="tab-nav text-center fw-nav">
            <li class="active"><a href="#tab1" data-toggle="tab" aria-expanded="true">Profile</a></li>
            <li><a href="#tab2" data-toggle="tab">Health</a></li>
            <li><a href="#tab3" data-toggle="tab">Diet</a></li>
            <li><a href="#tab4" data-toggle="tab">Movement</a></li>
            <li><a href="#tab5" data-toggle="tab">Environment</a></li>
            <li><a href="#tab6" data-toggle="tab">Rejuvenation</a></li>
            <li><a href="#tab7" data-toggle="tab">Spirit</a></li>
          </ul>

          <div class="tab-content" id="tabs">

            <div class="tab-pane fade active in" id="tab1">
                <%= render partial: 'users/intro' %>
                <%#= render partial: 'users/profile' %>
            </div>

            <div class="tab-pane fade" id="tab2">
                <%#= render partial: 'users/health' %>
            </div>

            <div class="tab-pane fade" id="tab3">
                <%#= render partial: 'users/strength', locals: { strategies: ['diet'] } %>
            </div>

            <div class="tab-pane fade" id="tab4">
                <%#= render partial: 'users/strength', locals: { strategies: ['movement'] } %>
            </div>

            <div class="tab-pane fade" id="tab5">
                <%#= render partial: 'users/strength', locals: { strategies: ['environment'] } %>
            </div>

            <div class="tab-pane fade" id="tab6">
                <%#= render partial: 'users/strength', locals: { strategies: ['rejuvenation'] } %>
            </div>

            <div class="tab-pane fade" id="tab7">
                <%#= render partial: 'users/strength', locals: { strategies: ['spirit'] } %>
            </div>

            <ul class="fw-footer pagination wizard">
              <li class="previous first disabled"><a class="a-prevent" href=""><i class="zmdi zmdi-more-horiz"></i></a></li>
              <li class="previous disabled"><a class="a-prevent" href=""><i class="zmdi zmdi-chevron-left"></i></a></li>
              <li class="next"><a class="a-prevent" href="/survey#tab2"><i class="zmdi zmdi-chevron-right"></i></a></li>
              <li class="next last"><a class="a-prevent" href=""><i class="zmdi zmdi-more-horiz"></i></a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
